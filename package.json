{
  "name": "ssb-mirror",
  "version": "0.0.1",
  "description": "A server to host and replicate one SSB feed only",
  "main": "index.js",
  "bin": "bin.js",
  "author": "Andre Staltz <contact@staltz.com>",
  "license": "AGPL-3.0",
  "scripts": {
    "build": "docker build -t staltz/ssb-mirror:$(jq -r .version package.json) .",
    "start": "docker run -it --rm staltz/ssb-mirror",
    "publish": "docker push staltz/ssb-mirror"
  },
  "dependencies": {
    "mkdirp": "~0.5.1",
    "multiserver": "~3.3.1",
    "ssb-client": "~4.7.1",
    "ssb-blobs": "~1.1.13",
    "ssb-config": "~3.2.5",
    "ssb-keys": "~7.1.5",
    "ssb-legacy-conn": "~1.0.23",
    "ssb-replicate": "~1.2.1",
    "ssb-server": "~14.1.9"
  }
}